{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "template": "template",
    "tutorials": "docs/",
    "readme": "../README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "staticFiles": {
        "include": ["./jsdoc/screen.css", "./jsdoc/script.js"]
      },
      "outputSourceFiles": true,
      "useLongnameInNav": true
    }
  },
  "docdash": {
    "mainpagetitle": "Introduction",
    "collapse": true,
    "search": true,
    "sectionOrder": ["Tutorials", "Classes", "Modules", "Mixins", "Interfaces"],
    "scripts": ["./jsdoc/screen.css", "./screen.css"]
  }
}
